{
    "configurations": [
        {
            "name": "Debug",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include"
            ],
            "defines": [
                "DEBUG",
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c11",
            "cppStandard": "${default}",
            "intelliSenseMode": "${default}"
        },
        {
            "name": "Release",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include"
            ],
            "defines": [
                "NDEBUG",
                "_NDEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c11",
            "cppStandard": "${default}",
            "intelliSenseMode": "${default}"
        }
    ],
    "version": 4
}
